javascript:var num=prompt("This is the Bittflood, This Will Flood Your History With A Page Of Your Choosing. How Times Do You Want This Page To Show Up In your History?\nMade By: SrBitt");done=false;x=window.__cpLocation.href;for (var i=1; i<=num; i++){history.pushState(0, 0, i==num?x:i.toString());if(i==num){done=true}}if(done===true){alert("History Flooding Successful!\n "+window.__cpLocation.href+" \nNow Appears In Your History "+num+(num==1?" time.":" Times. \nMade By: SrBitt"))}
